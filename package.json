{
  "name": "rollup-plugin-svelte-ssr",
  "version": "1.0.0",
  "description": "Server-side rendering of Svelte app at build-time using Rollup plugin",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akaSybe/rollup-plugin-svelte-ssr.git"
  },
  "keywords": [
    "svelte",
    "ssr",
    "rollup-plugin"
  ],
  "author": "Aleksandr Shestakov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akaSybe/rollup-plugin-svelte-ssr/issues"
  },
  "homepage": "https://github.com/akaSybe/rollup-plugin-svelte-ssr#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@types/jest": "^24.0.18",
    "del": "^5.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-svelte": "^5.1.0",
    "svelte": "^3.12.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/tests.js"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  }
